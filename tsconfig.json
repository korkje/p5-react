{
    "compilerOptions": {
        "target": "ESNext",
        "module": "ESNext",
        "lib": ["ESNext", "DOM"],
        "moduleResolution": "Bundler",
        "strict": true,
        "sourceMap": true,
        "isolatedModules": true,
        "noEmit": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "skipLibCheck": true,
        "baseUrl": "./src",
        "jsx": "react-jsx"
    },
    "include": ["src"]
}
